{
	"ReturnFormat": "JSON"
	,"RequestPatterns": {
		"/product": {
			"GET": {
				"Bean": "ProductService"
				,"Method": "getAllProducts"
			}
		}
		,"/product/{ProductID}": {
			"GET": {
				"Bean": "ProductService"
				,"Method": "getProductByID"
			}
			,"POST": {
				"Bean": "ProductService"
				,"Method": "saveProduct"
			}
		}
		,"/product/{ProductID}/colors": {
			"GET": {
				"Bean": "ProductService"
				,"Method": "getProductColors"
			}
		}
		,"/product/do/nothing": {
			"GET": {
				"Bean": "ProductService"
				,"Method": "returnNothing"
			}
		}
	}
}